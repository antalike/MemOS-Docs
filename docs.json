        "pages": [
          "/mintdocs/api-reference/configure/post",
          "/mintdocs/api-reference/chat/post",
          "/mintdocs/api-reference/search/post"
        ]
      }
    ]
  },
  "logo": {
    "light": "/mintdocs/logo/light.svg",
    "dark": "/mintdocs/logo/dark.svg",
    "href": "https://memos.openmem.net"
